{
  "name": "stackbrowseragent",
  "version": "1.0.0",
  "description": "Browser Agent with JWT Authentication and Extension",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "build:extension": "vite build",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:extension": "vite build --watch --mode development",
    "lint": "eslint src --ext .ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "keywords": [
    "browser",
    "automation",
    "agent",
    "jwt",
    "express",
    "typescript",
    "authentication",
    "api",
    "rest",
    "nodejs",
    "chrome-extension"
  ],
  "author": "stackconsult",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stackconsult/stackBrowserAgent.git"
  },
  "bugs": {
    "url": "https://github.com/stackconsult/stackBrowserAgent/issues"
  },
  "homepage": "https://github.com/stackconsult/stackBrowserAgent#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "API.md",
    "ARCHITECTURE.md",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "@types/ms": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "winston": "^3.11.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/node": "^20.19.25",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "sharp": "^0.34.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vite": "^7.2.2",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
